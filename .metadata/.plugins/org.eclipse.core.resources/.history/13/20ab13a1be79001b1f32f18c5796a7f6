package claseFile;

public class formateandoConPrintf {

	public static void main(String[]args) {
		
		
		System.out.printf("%-25s %-30s %6.2f\n","Hola que tal","Muy bien y tu?" , 25.00);
		System.out.printf("%-25s %-30s %f\n","Hola que ","Muy bien tu?" , 25.45);
		System.out.printf("%-25s %-30s %f\n","Hola qu","Muy bien y ?" , 25.34);
		System.out.printf("%-25s %-30s %f\n","Hola q","Muy y tu?" , 25.21);
		
		
		
	}
	
	
	
}
