public class ejercicio3 {
	
    public static void main(String[]args) {
	
	
}
    
    public static boolean contieneIrun(String cadena) {
    	boolean contiene=false;
    	     if(cadena.contains("Irun")) {
    	    	 contiene =true;
    	    	 return contiene ;}
    	     else {
    	    	 contiene=false;
    	    	 return contiene;
    	     }
    	
    }
    
}
